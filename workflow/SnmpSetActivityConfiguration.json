				"schema_id": "",
				"type": "[Util.prev_type]",
				"base_type": "[Util.prev_base_type]"
			},
			"action": {
				"id": "",
				"tap_id": "@Id.#",
				"ref_type": "action",
				"schema_id": "",
				"type": "snmp.set_request",
				"base_type": "activity",
				"name": "@DisplayName.#",
