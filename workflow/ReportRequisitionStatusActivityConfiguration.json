{
  "action": [
    {
      "previous_action": {
        "id": "[Util.build_ref(Util.prev_tap_id)]",
        "tap_id": "[Util.prev_tap_id]",
        "schema_id": "",
        "type": "[Util.prev_type]",
        "base_type": "[Util.prev_base_type]"
      },
      "action": {
        "id": "",
        "tap_id": "@Id.#",
        "ref_type": "action",
        "schema_id": "",
        "type": "psc.requisition_status",
        "base_type": "activity",
        "name": "@DisplayName.#",
        "title": "@DisplayName.#",
        "properties": {
          "action_timeout": "[Util.fix_interval_sec('@Configuration.Timeout.Period.#','@Configuration.Timeout.DisplayUnits.#')]{i}",
          "continue_on_failure": false,
          "skip_execution": false,
          "display_name": "@DisplayName.#",
          "description": "This uses requisition name but this is present in the CPO as it uses TaskName",
          "requisition_id": "@Configuration.RequisitionTaskName.#",
          "runtime_user": {
            "override_target_runtime_user": false,
            "target_default": true
          },
          "target": {
            "override_workflow_target": true,
            "target_id": "[Util.build_ref('@Configuration.TargetLoadBalancer.HardcodedValue.Id.#@Configuration.TargetLoadBalancer.SingleTargetReference.HardcodedValue.Id.#')]"
          }
        }
      }
    }
  ]
}